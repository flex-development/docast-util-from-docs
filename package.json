      "optional": true
    },
    "@types/unist": {
      "optional": true
    },
    "micromark-util-types": {
      "optional": true
    }
  },
  "resolutions": {
    "chai": "5.1.0"
  },
  "engines": {
    "node": ">=18.18.2"
  },
  "packageManager": "yarn@4.1.0",
  "sideEffects": false
}
